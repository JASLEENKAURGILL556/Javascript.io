//switch statement
function switches(val)
{
  switch(val)
    {
      case 1:
        console.log("it is 1");
        break;
      case 2:
        console.log("it is 2");
        break;
      default:
        console.log("out of range");
        break;
    }
}

switches(1);

//sequenctial switch statement
function switches(val)
{
  switch(val)
    {
      case 1:
      case 2:
        console.log("it is under 3");
        break;
      case 3:
        console.log("it is 3");
        break;
      default:
        console.log("it is over 3");
        break;
    }
}

switches(10);
